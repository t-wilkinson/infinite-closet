{
  "kind": "collectionType",
  "collectionName": "review",
  "info": {
    "name": "Review",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "order": {
      "model": "order",
      "plugin": "orders",
      "required": false
    },
    "fit": {
      "type": "enumeration",
      "enum": ["small", "true", "large"]
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "heading": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "string",
      "required": true
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["images", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
