{
  "kind": "collectionType",
  "collectionName": "shipments",
  "info": {
    "name": "Shipment",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": ["normal", "delayed"],
      "default": "normal"
    },
    "shipmentId": {
      "type": "string"
    },
    "shippingClass": {
      "type": "enumeration",
      "enum": ["one", "two"]
    },

    "position": {
      "type": "enumeration",
      "enum": ["confirmed", "shipped", "start", "end", "cleaning", "completed"],
      "default": "confirmed"
    },
    "confirmed": {
      "type": "datetime"
    },
    "shipped": {
      "type": "datetime"
    },
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "cleaning": {
      "type": "datetime"
    },
    "completed": {
      "type": "datetime"
    }
  }
}
